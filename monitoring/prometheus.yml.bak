global:
  scrape_interval: 5s
  evaluation_interval: 5s

scrape_configs:
  - job_name: mosquitto
    static_configs:
      - targets: ['mosquitto-exporter:9234']

  - job_name: cadvisor
    static_configs:
      - targets: ['cadvisor:8080']
      
  - job_name: 'victim'
    static_configs:
      - targets: ['host.docker.internal:8000']

  - job_name: 'c2'
    static_configs:
      - targets: ['host.docker.internal:5000']

    