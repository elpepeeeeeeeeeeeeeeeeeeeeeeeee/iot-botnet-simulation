  "panels": [
    {
      "type": "timeseries",
      "title": "MQTT Messages Received (per sec)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(mosquitto_messages_received_total[1m])",
          "legendFormat": "received/s",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "MQTT Messages Sent (per sec)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(mosquitto_messages_sent_total[1m])",
          "legendFormat": "sent/s",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Connected Clients (now)",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "mosquitto_clients_connected",
          "legendFormat": "clients",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "none" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Bytes Received (per sec)",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(mosquitto_bytes_received_total[1m])",
          "legendFormat": "bytes_in/s",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Bytes Sent (per sec)",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 8 },
